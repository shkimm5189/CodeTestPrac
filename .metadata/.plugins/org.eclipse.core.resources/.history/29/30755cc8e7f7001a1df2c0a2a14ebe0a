package b1018;

import java.util.Scanner;

public class Main {
	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		int cnt=0;
		int n = sc.nextInt();
		int m = sc.nextInt();
		
		char[][] board = new char[n][m];
		
		for(int i =0 ; i < n; i++ ) {
			String str = sc.next();
			for(int j = 0; j < m; j++) {
				board[i][j] = str.charAt(j);
			}
		}
		if(board[0][0] == 'W') {
			for(int i =0 ; i < n; i++ ) {	
				for(int j = 0; j < m; j+=2) {
					if(board[i][j] != 'W') {
						cnt++;
					System.out.println("A");
					}
				}
			}
			for(int i =1 ; i < n; i++ ) {	
				for(int j = 1; j < m; j+=2) {
					if(board[i][j] != 'B') {
						cnt++;
					System.out.println("B");
					}
				}
			}
		} else if(board[0][0] == 'B') {
			for(int i =0 ; i < n; i++ ) {	
				for(int j = 0; j < m; j+=2) {
					if(board[i][j] != 'B') {
						cnt++;
					System.out.println("C");
					}
				}
			}
			for(int i =0 ; i < n; i++) {	
				for(int j = 1; j < m; j+=2) {
					if(board[i][j] != 'W') {
						cnt++;
						System.out.println("D");
					}
				}
			}
		}
		System.out.println(cnt);
	
	}
}
