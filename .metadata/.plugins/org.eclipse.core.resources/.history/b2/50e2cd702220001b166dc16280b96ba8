package 비밀지도1차;

public class Solution {
    public String[] solution(int n, int[] arr1, int[] arr2) {
        String[] answer = {};
        String[] arrs1 = new String[arr1.length];
        String[] arrs2 = new String[arr2.length];
        for(int i = 0 ; i < arr1.length; i++) {
        	arrs1[i] = Integer.toBinaryString(arr1[i]);
        }
        for(int i = 0 ; i < arr1.length; i++) {
        	arrs2[i] = Integer.toBinaryString(arr1[i]);
        }
        
        return answer;
    }
}
