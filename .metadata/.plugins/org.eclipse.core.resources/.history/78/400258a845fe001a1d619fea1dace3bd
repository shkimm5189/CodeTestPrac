package number1;

import java.awt.List;
import java.util.ArrayList;
import java.util.Arrays;

class Solution {
	public int[] solution(int[] answers) {
        int[] answer = {};
        int[] ans = {0,0,0};
        int[] stu1 = {1,2,3,4,5};
        int[] stu2 = {2, 1, 2, 3, 2, 4, 2, 5};
        
        int[] stu3 = {3, 3, 1, 1, 2, 2, 4, 4, 5, 5};
        int s1=0;
        int s2=0;
        int s3=0;
        
        
        for(int i =0  ; i < answers.length-1; i++ ) {
        	//stu1
        	if(answers[i] == stu1[s1]) {
        		ans[0]++;
        	}
        	s1++;
        	if(s1 == stu1.length-1) {
        		s1=0;
        	}
        	
        	//stu2
        	if(answers[i] == stu2[s2]) {
        		ans[1]++;
        	}
        	s2++;
        	if(s2 == stu2.length-1) {
        		s2=0;
        	}
        	
        	//stu3
        	if(answers[i] == stu3[s3]) {
        		ans[2]++;
        	}
        	s3++;
        	if(s3 == stu3.length-1) {
        		s3=0;
        	}
        }
        int max = Math.max(Math.max(ans[0],ans[1]),ans[2]);
        ArrayList<Integer> stu = new ArrayList<>();
        if(max == ans[0]) {
        	stu.add(1);
        }
        if(max == ans[1]) {
        	stu.add(2);
        }
        if(max == ans[2]) {
        	stu.add(3);
        }
        for(int i= 0 ;i<stu.size();i++) {
        	answer[i] = stu.get(i);
        }
        return answer;
        
    }
}