package b1018;

import java.util.Scanner;

public class Main {
	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		int cnt=0;
		int n = sc.nextInt();
		int m = sc.nextInt();
		int sn=0;
		int sm=0;
		
		char[][] board = new char[n][m];

		for(int i =0 ; i < n; i++ ) {
			String str = sc.next();
			for(int j = 0; j < m; j++) {
				board[i][j] = str.charAt(j);
			}
		}
		if (n > 8 ) {
			n -= 8;
		}
		if (m > 8) {
			m -= 8;
		}
		for(int i = 0 ; i < sn;i++) {
			for(int j = 0;j < sm ;j++) {
				System.out.println("asd");
			}
		}
		System.out.println(chkChess(n, m, board));
	}
	public static int chkChess(int n, int m,char[][] board) {
		int cnt = 0;
		if(board[0][0] == 'W') {
			for(int i = 0 ; i < 8; i+=2) {
				for(int j = 0; j < 8; j+=2) { //짝수
					if(board[i][j] != board[0][0])
						cnt++;
				}
				for(int j = 1; j < 8; j+=2) { // 홀수
					if(board[i][j] != board[0][1]) 
						cnt++;
				}	
			}
			for(int i = 1 ; i < 8; i+=2) {
				for(int j = 0; j < m; j+=2) { //짝수
					if(board[i][j] != board[1][0])
						cnt++;
				}
				for(int j = 1; j < 8; j+=2) { // 홀수
					if(board[i][j] != board[1][1]) 
						cnt++;
				}

			}
		}else if(board[0][0] == 'B') {
			for(int i = 0 ; i < 8; i+=2) {
				for(int j = 0; j < 8; j+=2) { //짝수
					if(board[i][j] != board[0][0])
						cnt++;
				}
				for(int j = 1; j < 8; j+=2) { // 홀수
					if(board[i][j] != board[0][1]) 
						cnt++;
				}	
			}
			for(int i = 1 ; i < 8; i+=2) {
				for(int j = 0; j < m; j+=2) { //짝수
					if(board[i][j] != board[1][0])
						cnt++;
				}
				for(int j = 1; j < 8; j+=2) { // 홀수
					if(board[i][j] != board[1][1]) 
						cnt++;
				}

			}
		}
		return cnt;
	}
	
}


