package 비밀지도1차_recheck;

import java.util.HashMap;

public class Solution {
    public static String[] solution(int n, int[] arr1, int[] arr2) {
        String[] answer = new String[n];
        String[] arrs1 = new String[arr1.length];
        String[] arrs2 = new String[arr2.length];
        
        
        for(int i = 0 ; i < arr1.length; i++) {
        	arrs1[i] = Integer.toBinaryString(arr1[i] &arr2[i]);
        }
        for(int i = 0 ; i < arr2.length; i++) {
        	arrs2[i] = Integer.toBinaryString(arr2[i]);
        }
        for(int i = 0 ; i < arr1.length; i++) {
        	System.out.println(arrs1[i]);
        }
        
        
        return answer;
    }
  
    public static void main(String[] args) {
    	int[] a = {9, 20, 28, 18, 11};
    	int[] b = {30, 1, 21, 17, 28};
		System.out.println(solution(5, a, b));
	}
}
