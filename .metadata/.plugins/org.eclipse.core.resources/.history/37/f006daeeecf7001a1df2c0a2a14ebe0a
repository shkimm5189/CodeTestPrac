package b1018;

import java.util.Scanner;

public class Main {
	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		int cnt=0;
		int n = sc.nextInt();
		int m = sc.nextInt();
		
		char[][] board = new char[n][m];
		
		for(int i =0 ; i < n; i++ ) {
			String str = sc.next();
			for(int j = 0; j < m; j++) {
				board[i][j] = str.charAt(j);
			}
		}
		if(board[0][0] == 'W') {
			for(int i = 0 ; i < n; i+=2) {
				for(int j = 0; j < n; j+=2) { //짝수
					if(board[i][j] != 'W')
						cnt++;
				}
				for(int j = 1; j < n; j+=2) { // 홀수
					if(board[i][j] != 'B') 
						cnt++;
				}	
			}
			for(int i = 1 ; i < n; i+=2) {
				for(int j = 0; j < n; j+=2) { //짝수
					if(board[i][j] != 'B')
						cnt++;
				}
				for(int j = 1; j < n; j+=2) { // 홀수
					if(board[i][j] != 'W') 
						cnt++;
				}
				
			}
		}
		if(board[0][0] == 'B') {
			for(int i = 0 ; i < n; i+=2) {
				for(int j = 0; j < n; j+=2) { //짝수
					if(board[i][j] != 'B')
						cnt++;
				}
				for(int j = 1; j < n; j+=2) { // 홀수
					if(board[i][j] != 'W') 
						cnt++;
				}	
			}
			for(int i = 1 ; i < n; i+=2) {
				for(int j = 0; j < n; j+=2) { //짝수
					if(board[i][j] != 'W')
						cnt++;
				}
				for(int j = 1; j < n; j+=2) { // 홀수
					if(board[i][j] != 'B') 
						cnt++;
				}
				
			}
	}
}

